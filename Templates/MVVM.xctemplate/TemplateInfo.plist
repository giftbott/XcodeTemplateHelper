<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>MVVM Module</string>
	<key>Description</key>
	<string>Creates basic view and viewModel template for MVVM</string>
	<key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.swift</string>
	<key>SortOrder</key>
	<integer>90</integer>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>Name of the module to create</string>
			<key>Identifier</key>
			<string>MVVMModuleName</string>
			<key>Name</key>
			<string>Module name</string>
			<key>Default</key>
			<string>ModuleName</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>Input Module Name</string>
			<key>Required</key>
			<true/>
			<key>NotPersisted</key>
			<true/>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_MVVMModuleName___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Type</key>
			<string>static</string>
		</dict>
	</array>
	<key>Template Author</key>
	<string>giftbott</string>
	<key>Summary</key>
	<string>This generates a new module using MVVM architecture.</string>
</dict>
</plist>
